  <!-- Log4Net Section -->
  <log4net>
    <appender name="ApplicationLayerCompositeAppender" type="log4net.Appender.RollingFileAppender">
      <file value="D:\Logs\WebApiLog.txt" />
      <appendToFile>true</appendToFile>
      <rollingStyle value="Composite" />
      <datePattern value="yyyy-MM-dd" />
      <maxSizeRollBackups value="10" />
      <preserveLogFileNameExtension value="true" />
      <maximumFileSize value="10MB" />

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%timestamp %date %level - %message%newline%newline" />
      </layout>
    </appender>

    <appender name="ErrorCompositeAppender" type="log4net.Appender.RollingFileAppender">
      <file value="D:\Logs\WebApiErrorLog.txt" />
      <appendToFile>true</appendToFile>
      <rollingStyle value="Composite" />
      <datePattern value="yyyy-MM-dd" />
      <maxSizeRollBackups value="10" />
      <preserveLogFileNameExtension value="true" />
      <maximumFileSize value="10MB" />

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%timestamp %date %level - %message%exception%newline%newline" />
      </layout>
    </appender>

    <root>
      <level value="ALL" />
      <appender-ref ref="ApplicationLayerCompositeAppender" />
    </root>

    <logger additivity="false" name="Cube.INX.Core.WebApi.Exceptions.WebApiExceptionFilterAttribute">
      <level value="ERROR" />
      <appender-ref ref="ErrorCompositeAppender" />
    </logger>
  </log4net>
